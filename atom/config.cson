"*":
  Hydrogen:
    autoScroll: false
    autocomplete: false
    languageMappings: "{\"R\":\"RMarkdown\"}"
    outputAreaDock: true
    outputAreaFontSize: 18
    showAutocompleteFirst: false
  "atom-beautify":
    general:
      beautifyEntireFileOnSave: false
      loggerLevel: "info"
    latex:
      always_look_for_split_braces: false
      remove_trailing_whitespace: true
  "atom-latex":
    compiler: "xelatex"
    delayed_minimap_refresh: true
    file_ext_to_clean: "*.aux, *.bbl, *.blg, *.idx, *.ind, *.lof, *.lot, *.out, *.toc, *.acn, *.acr, *.alg, *.glg, *.glo, *.gls, *.ist, *.fls, *.log, *.fdb_latexmk, *.bcf, *.mw, *.run.xml, *.synctex.gz"
    hide_log_if_success: true
    latexmk_param: "-pdf"
  "atom-package-deps":
    ignored: []
  "autocomplete-clang":
    includePaths: [
      "."
      "./include"
      "../include"
    ]
    includeSystemHeadersDocumentation: true
  "autocomplete-latex":
    enableBuiltinProvider: true
  "autocomplete-plus":
    fileBlacklist: [
      "*.md"
      "*.txt"
    ]
  "autocomplete-python":
    extraPaths: "$PROJECT/venv/lib/python3.10/site-packages"
    pythonPaths: "$PROJECT/venv/bin/python3.10"
    useKite: false
    useSnippets: "required"
  "bracket-matcher": {}
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "language-gfm"
      "markdown-preview"
      "hidpi"
      "linter-clang"
      "linter-markdown"
      "autocomplete-R"
      "ide-r"
      "atom-ide-ui"
      "linter-lintr"
      "language-clips"
      "linter-scalac"
      "spell-check"
      "linter-mypy"
    ]
    projectHome: "/home/antcc/"
    telemetryConsent: "no"
    themes: [
      "nord-atom-ui"
      "nord-atom-syntax"
    ]
  "docblock-python": {}
  docblockr:
    align_tags: "shallow"
    extend_double_slash: false
  editor:
    fontFamily: "Fira Code"
    fontSize: 15
    preferredLineLength: 100
    scrollPastEnd: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    tabType: "soft"
  "exception-reporting":
    userId: "4c25c82f-d5ef-8215-3f42-253a818b6d69"
  "ftp-remote-edit":
    config: ""
    password: "3ba188020d735eb7d06534"
  hidpi:
    manualResolutionScaleFactors: "\"1920x1080\": 1.20"
    reopenCurrentFile: true
    scaleFactor: 1.2
    startupDelay: 100
  "hydrogen-launcher":
    app: "tilix"
  "ide-python":
    pylsPlugins:
      flake8: {}
      pylint:
        enabled: true
  "jupyter-notebook":
    jupyterPath: "usr/bin/jupyter"
  kite:
    enableHoverUI: false
    showWelcomeNotificationOnStartup: false
    startKiteAtStartup: false
  "latex-autocomplete": {}
  linter:
    lintOnChange: false
  "linter-chktex":
    chktexArgs: [
      "-wall"
      "-n22"
      "-n30"
      "-e16"
      "-n3"
      "-n21"
      "-n1"
      "-n12"
    ]
  "linter-clang":
    clangIncludePaths: [
      ". ./include"
    ]
  "linter-flake8":
    executablePath: "$PROJECT/.venv/bin/flake8;/home/antcc/.local/bin/flake8"
    ignoreErrorCodes: [
      "E226"
      "D202"
      "D103"
      "D100"
      "W503"
    ]
    pycodestyleErrorsToWarnings: true
  "linter-gcc":
    gccDefaultCFlags: "-c -Wall-fsyntax-only"
    gccDefaultCppFlags: "-c -Wall -std=c++14 -fsyntax-only"
    gccIncludePaths: " -../include/, include/, ../include/,"
  "linter-mypy":
    disallowAnyExpr: false
    disallowIncompleteDefs: false
    disallowUntypedCalls: false
    disallowUntypedDefs: false
    disallowUntypedGlobals: false
    ignoreFiles: "test.+"
    warnNoReturn: false
  "linter-python-pep8":
    pep8DirToExecutable: "~/.local/bin/"
  "linter-rust":
    cargoCommand: "build"
    cargoPath: "/usr/bin/cargo"
    rustcPath: "/usr/bin/rustc"
  "linter-scalac":
    lintsOnChange: true
  "linter-spell":
    defaultLanguages: [
      "en-US"
    ]
    spellPath: "/usr/bin/hunspell"
    spellingErrorSeverity: "info"
  "linter-spell-latex":
    words: [
      "a4paper"
      "fontsize"
      "widepar"
      "scrheadings"
      "kaobook"
      "11pt"
      "twoside"
      "chapterprefix"
      "chapterentrydots"
      "secnumdepth"
      "noenddot"
      "enddot"
      "overfullrule"
      "komascript"
      "kaohandt"
      "newcmmath"
      "otf"
      "authoryear"
      "kaobiblio"
      "pdfauthor"
      "pdftitle"
      "citecolor"
      "urlcolor"
      "anchorcolor"
      "linkcolor"
      "margintoc"
      "margintocdepth"
      "eqref"
      "eqref"
      "subsec"
      "bibnote"
      "prenote"
      "tex"
      "mle"
    ]
  "linter-ui-default":
    panelHeight: 300
  "markdown-image-paste":
    subfolder: "img"
    use_subfolder: true
  "markdown-preview-enhanced":
    exportPDFPageFormat: "A4"
  minimap:
    adjustMinimapWidthToSoftWrap: false
    moveCursorOnMinimapClick: true
  "pdf-view":
    reverseSyncBehaviour: "Double click"
  "platformio-ide-terminal":
    ansiColors:
      normal:
        blue: "#0061cd"
    core:
      mapTerminalsTo: "Folder"
      shell: "/bin/zsh"
      workingDirectory: "Active File"
    style:
      theme: "pro"
  "preview-inline":
    imageBasePath: "img"
  "python-autopep8":
    maxLineLength: 79
  "python-black":
    fmtOnSave: false
    skipStringNormalization: true
  racer:
    racerBinPath: "~/.cargo/bin/racer"
    rustSrcPath: '''
      ~/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/lib/rustlib/src/rust/src/
      
    '''
  "spell-check":
    grammars: [
      "text.md"
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "source.rst"
      "text.restructuredtext"
      "text.tex.latex"
      "source.python"
      ""
    ]
    locales: [
      "en-US"
      "es-ES"
    ]
  teletype: {}
  welcome:
    showOnStartup: false
".c.source":
  editor:
    autoIndentOnPaste: false
".cpp.source":
  editor:
    autoIndentOnPaste: false
".latex.tex.text":
  "bracket-matcher":
    autocompleteCharacters: [
      "()"
      "[]"
      "{}"
      "<>"
      "«»"
      "`'"
    ]
".rust.source":
  editor:
    preferredLineLength: 99
